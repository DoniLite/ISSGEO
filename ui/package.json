{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --hot src/index.tsx",
    "generate:watch": "bun --watch generate",
    "start": "NODE_ENV=production bun src/index.tsx",
    "build": "bun build:client && bun build:server",
    "build:all": "bun build && bun build:default",
    "build:default": "bun run build.ts --outdir=dist/default --sourcemap=linked ",
    "build:client": "bun run build.ts --outdir=dist/client --minify --sourcemap=linked --target=browser --entrypoints=./src/ssr/client.tsx,",
    "build:server": "bun run build.ts --outdir=dist/server --sourcemap=external --splitting --format=esm --target=node --entrypoints=./src/ssr/server.tsx,",
    "generate": "bunx @tanstack/router-cli generate"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-router": "^1.131.27",
    "@tanstack/router-devtools": "^1.131.27",
    "bun-plugin-tailwind": "^0.0.15",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-auto-scroll": "^8.6.0",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "i18next": "^25.4.2",
    "lucide-react": "^0.541.0",
    "react": "^19",
    "react-day-picker": "^9.9.0",
    "react-dom": "^19",
    "react-i18next": "^15.7.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.4",
    "@tanstack/router-cli": "^1.131.27",
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19"
  }
}
